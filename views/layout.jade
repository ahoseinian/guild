doctype html
html
  head
    block title
      title= page.title
      meta(name='description', content=page.desc)

    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
    meta(http-equiv='x-ua-compatible', content='ie=edge')
    link(rel='stylesheet', href='http://localhost:8080/assets/css/vendor.css')
    block links
  body
    include ./components/nav
    block content

    script(src='http://localhost:8080/assets/vendor.js')
    block scripts
    